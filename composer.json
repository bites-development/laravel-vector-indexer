{
    "name": "bites/laravel-vector-indexer",
    "description": "Automatic vector indexing and semantic search for Laravel models using OpenAI embeddings and Qdrant",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "vector",
        "search",
        "semantic",
        "embeddings",
        "openai",
        "qdrant",
        "ai"
    ],
    "authors": [
        {
            "name": "Bites Team",
            "email": "dev@bites.app"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "illuminate/database": "^9.0|^10.0|^11.0",
        "illuminate/console": "^9.0|^10.0|^11.0",
        "guzzlehttp/guzzle": "^7.0",
        "openai-php/client": "^0.8|^0.9|^0.10"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Bites\\VectorIndexer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bites\\VectorIndexer\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bites\\VectorIndexer\\Providers\\VectorIndexerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
